    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-turbo": "^1.10.15",
    "prettier": "latest",
    "turbo": "latest"
  },
  "engines": {
    "node": ">=18.17.1"
  },
  "packageManager": "pnpm@8.9.0"
}